<Window x:Class="MaSH.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaSH"
        mc:Ignorable="d"
        Title="MaSH Processing Log" Height="375" Width="600" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" Icon="MaSH.ico" Closing="Window_Closing">
    <StackPanel>
        <ListBox x:Name="listLog" Height="312"/>
        <Label Content="Processing MaSH Schedule"/>
    </StackPanel>
</Window>
